    <script>
        function RegistrationCall() {
            $(".req").each(function () {
                $(this).css("border", "grey solid 1px");
            });
            if (Validations(false, "regEnq")) {
                var FirstName = $("#FirstName").val();
                var LastName = $("#LastName").val();
                var Contact = $("#Number").val();
                var EmailId = $("#Email").val();
                var CountryId = $("#CountryList").val();
                var Status = 0;
                if ($('#chkterm').is(":checked")) {
                    Status = 1;
                }
                var data = { "FirstName": FirstName, "LastName": LastName, "Contact": Contact, "EmailId": EmailId, "CountryId": CountryId, "Status": Status };
                $.ajax({
                    url: 'https://Expert Global Miners.com/trade/',
                    data: { "Data": JSON.stringify(data) },
                    cache: false,
                    async: false,
                    crossDomain: true,
                    success: function (data) {
                        alert(data)
                    },
                    error: function (data) {
                        alert('Request Recieved Successfully');
                        location.reload();
                    }
                });

            }



        }
    </script>

